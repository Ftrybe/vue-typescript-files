
{
	"if": {
		"prefix": "if",
		"body": "{{#if ${1:condition}}}\n\t${0}\n{{/if}}",
		"description": "IF"
	},
	"ifel": {
		"prefix": "ifel",
		"body": "{{#if ${2:condition}}}\n\t${1}\n{{else}}\n\t${0}\n{{/if}}",
		"description": "IF ELSE"
	},
	"each": {
		"prefix": "each",
		"body": "{{#each ${1:list} as |${2:alias}|}}\n\t${0}\n{{/each}}",
		"description": "EACH"
	},
	"inc" : {
		"prefix": "inc",
		"body": "inc ${1:number}}",
		"description": "INC"
	},
	"seq_contains": {
